<main class="">
    <h1>{{header}}</h1>
    <section class="row">
        <div class="col-8">
            {{#with contacts_data}}
            <div class="mt-5">
                <h3>{{title}}</h3>
                <div class="mt-3">
                    {{#each buttons}}
                    <a href="" class="btn {{#if isPrimary}} btn-primary {{/if}} {{#if isOutline}} btn-outline-primary ms-2 {{/if}}">{{text}}</a>
                    {{/each}}
                </div>
                <div class="row">
                    {{#with form.input}}
                    <div class="col-6 mt-3">
                        {{#with surname}}
                        <div>
                            <label for="" class="form-label">{{label}}</label>
                            <input type="text" placeholder="{{placeholder}}" class="form-control">
                            <div class="form-text">{{caption}}</div>
                        </div>
                        {{/with}}
                    </div>
                    <div class="col-6 mt-3">
                        {{#with name}}
                        <div>
                            <label for="" class="form-label">{{label}}</label>
                            <input type="text" placeholder="{{placeholder}}" class="form-control">
                            <div class="form-text">{{caption}}</div>
                        </div>
                        {{/with}}
                    </div>
                    <div class="col-6 mt-3">
                        {{#with phone}}
                        <div>
                            <label for="" class="form-label">{{label}}</label>
                            <input type="text" placeholder="{{placeholder}}" class="form-control">
                            <div class="form-text">{{caption}}</div>
                        </div>
                        {{/with}}
                    </div>
                    <div class="col-6 mt-3">
                        {{#with email}}
                        <div>
                            <label for="" class="form-label">{{label}}</label>
                            <input type="text" placeholder="{{placeholder}}" class="form-control">
                            <div class="form-text">{{caption}}</div>
                        </div>
                        {{/with}}
                    </div>
                    <hr class="mt-5">
                    {{/with}}
                    {{#each form.switch}}
                    <div class="form-check form-switch mt-3 ms-3">
                        {{#unless isActive}}
                        <input type="checkbox" class="form-check-input">
                        {{else}}
                        <input checked type="checkbox" class="form-check-input">
                        {{/unless}}
                        <label for="form-check-label">{{text}}</label>
                    </div>
                    {{/each}}
                    <hr class="mt-5">
                </div>
                {{/with}}
            </div>
            <div>
                {{#with delivery}}
                <h3 class="mt-3">{{title}}</h3>
                <div class="row">
                    {{#each select}}
                    <div class="card col mt-3">
                        <div>
                            <label for="" class="form-label">{{label}}</label>
                            <select class="form-select" title="{{placeholder}}" name="" id="">
                                <option value="">{{placeholder}}</option>
                                {{#each options}}
                                <option value="{{value}}">{{text}}</option>
                                {{/each}}
                            </select>
                        </div>
                    </div>
                    {{/each}}
                </div>
                <div class="mt-3">
                    <div class="row">
                        <div class="form-check">
                            {{#each delivery_type}}
                            <div>
                                <input type="radio" name="" id="" class="">
                                <label for="" class="form-check-label">{{type}}</label>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
                <hr>
                <div class="mt-3">
                    {{#with payment}}
                    <h3>{{title}}</h3>
                    <div class="row">
                        <div class="form-check">
                            {{#each list}}
                            <div>
                                <input type="radio" name="" id="">
                                <label for="" class="form-check-label">{{type}}</label>
                                {{#if @first}}
                                <div>
                                    <label class="form-control-sm">{{description}}</label>
                                </div>
                                {{/if}}
                            </div>
                            {{/each}}
                        </div>
                    </div>
                    {{/with}}
                    <hr>
                </div>
                <div class="h-3">
                    {{#with contact}}
                    <div class="row">
                        <div class="col-6 mt-3">
                            {{#with surname}}
                            <div>
                                <label for="" class="form-label">{{label}}</label>
                                <input type="text" placeholder="{{placeholder}}" class="form-control">
                                <div class="form-text">{{caption}}</div>
                            </div>
                            {{/with}}
                        </div>
                        <div class="col-6 mt-3">
                            {{#with name}}
                            <div>
                                <label for="" class="form-label">{{label}}</label>
                                <input type="text" placeholder="{{placeholder}}" class="form-control">
                                <div class="form-text">{{caption}}</div>
                            </div>
                            {{/with}}
                        </div>
                        <div class="col-6 mt-3">
                            {{#with phone}}
                            <div>
                                <label for="" class="form-label">{{label}}</label>
                                <input type="text" placeholder="{{placeholder}}" class="form-control">
                                <div class="form-text">{{caption}}</div>
                            </div>
                            {{/with}}
                        </div>
                        <div class="col-6 mt-3">
                            {{#with email}}
                            <div>
                                <label for="" class="form-label">{{label}}</label>
                                <input type="text" placeholder="{{placeholder}}" class="form-control">
                                <div class="form-text">{{caption}}</div>
                            </div>
                            {{/with}}
                        </div>
                    </div>
                    {{/with}}
                    <hr>
                    <div class="row">
                        {{#with coment}}
                        <label class="form-label" for="">{{label}}</label>
                        <input type="text" placeholder="{{placeholder}}" class="form-control ps-3">
                        {{/with}}
                    </div>
                    <hr class="mt-5">
                </div>
                {{/with}}
            </div>
            <section>
                {{#with coupon}}
                <h3>{{title}}</h3>
                {{#with select}}
                <div>
                    {{#with availabel_coupon}}
                    <label for="" class="form-label">{{label}}</label>
                    <div class="row">
                        <select name="" id="" class="form-select">
                            <option value="">{{placeholder}}</option>
                            {{#each options}}
                            <option value="{{value}}">{{text}}</option>
                            {{/each}}
                        </select>
                    </div>
                    {{/with}}
                    {{#with friend_coupon}}
                    <label for="" class="form-label mt-3">{{label}}</label>
                    <div class="row">
                        <select name="" id="" class="form-select">
                            <option value="">{{placeholder}}</option>
                            {{#each options}}
                            <option value="{{value}}">{{text}}</option>
                            {{/each}}
                        </select>
                    </div>
                    {{/with}}
                </div>
                {{/with}}
                <section>
                    {{#with add}}
                    <div>
                        {{#with friend}}
                        <div class="row">
                            <label for="" class="form-label">{{label}}</label>
                            <input type="text" placeholder="{{placeholder}}" class="form-control">
                            <div class="form-text">{{caption}}</div>
                        </div>
                        {{/with}}
                    </div>
                    <div class="mt-3">
                        {{#with colleague}}
                        <div class="row">
                            <label for="" class="form-label">{{label}}</label>
                            <input type="text" placeholder="{{placeholder}}" class="form-control">
                            <div class="form-text">{{caption}}</div>
                        </div>
                        {{/with}}
                    </div>

                    {{/with}}
                </section>
                {{/with}}
                <hr>
            </section>
            <section>
                <div class="mt-3">
                    {{#with quiz}}
                    <h3>{{title}}</h3>
                    <div class="row">
                        <div class="form-check col-12">
                            {{#each preference.options}}
                            <div>
                                <input type="radio" name="" id="">
                                <label for="" class="form-check-label">{{this}}</label>
                            </div>
                            {{/each}}
                        </div>
                        <div class="row mt-3">
                            <label for="" class="form-label">{{your_expirience.label}}</label>
                            <input type="text" class="form-input">
                        </div>
                        {{#with purchase_frequency}}
                        <div class="row mt-3">
                            <label for="form-label">{{label}}</label>
                            <select name="" id="" class="form-control-lg">
                                <option class="" value="">{{placeholder}}</option>
                                {{#each options}}
                                <option value="{{value}}">{{text}}</option>
                                {{/each}}
                            </select>
                            {{/with}}
                            {{#with factors}}
                            <div class="ms-1 mt-3">
                                <div> <label for="" class="form-check-lable">{{title}}</label></div>
                                {{#each options}}
                                <div class="form-check">
                                    <label for="" class="form-check-lable">{{this}}</label>
                                    <input name="food" type="radio" class="form-check-input">
                                </div>
                                {{/each}}
                            </div>
                            {{/with}}
                        </div>

                    </div>
                    {{/with}}

                </div>
            </section>
        </div>
        <div class="col-4">
            <div class="card">
                {{#each action_block}}
                <div class="mt-2 ms-1 {{#if @last}} mb-2{{/if}}">
                    <a href="" class="btn {{#if isSuccess}} btn-primary {{/if}} {{#if isDanger}} btn-danger {{/if}} {{#if isInfo}} btn-info {{/if}}">{{text}}</a>
                </div>
                {{/each}}
            </div>
            <div class="card mt-2">
                {{#with total}}
                <h3 class="ms-1">
                    {{title}}
                </h3>
                <div class="card-title ms-1">
                    {{#each item_list}}
                    <div>{{name}}: {{price}}</div>
                    {{/each}}
                    <hr>
                </div>
                <div class="card-title ms-1">
                    {{#each additional_info}}
                    <div>{{text}}: {{value}}</div>
                    {{/each}}
                    <hr>
                </div>
                <div class="card-title ms-1">
                    {{#with total_price}}
                    <div>{{text}}: {{value}}</div>
                    {{/with}}
                    <hr>
                </div>
                <div class="card-title ms-1">
                    {{#each button}}
                    <a href="#" class="btn mt-2 {{#if isPrimary}} btn-primary {{/if}} {{#if isOutline}} btn-outline-primary {{/if}}">{{text}}</a>
                    {{/each}}
                </div>
                {{/with}}
            </div>
        </div>
    </section>
</main>