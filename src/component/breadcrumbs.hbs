<div class="mt-5 ms-4">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            {{#each breadcrumb}}
            {{#if url}}
            <li class="breadcrumb-item"><a href="{{url}}">{{name}}</a></li>
            {{else}}
            <li class="breadcrumb-item active">{{name}}</li>
            {{/if}}
            {{/each}}
        </ol>
    </nav>
</div>